    <add key="HtmlTemplateBuild" value="&lt;p style=&quot;font-family:Trebuchet MS;font-size:18&quot;&gt; &lt;br/&gt; Build definition: &lt;b&gt; {buildDefinition} &lt;/b&gt; &lt;br/&gt; Last build {lastBuildName} is available by Uri: {lastBuildUri} &lt;br/&gt; Test Run: {testRunTitle} &lt;br/&gt; Requested by: {RequestedBy} &lt;br/&gt; Build Configuration: {BuildConfiguration} &lt;br/&gt; Test Run initiated at: {DateStarted} | Test Run Completed: {DateCompleted} &lt;br/&gt; Total tests: {totalTests} &lt;br/&gt; &lt;/p&gt;"/>
    <add key="HtmlTemplateAttachment" value="&lt;div style=&quot;font-family:Trebuchet MS;font-size:16;color:green&quot;&gt; Detailed Test Run report is available by: {attachUri} &lt;br/&gt; &lt;/div&gt;"/>

    <add key="HtmlTemplatePassedTestsTitle" value="&lt;div style=&quot;font-family:Trebuchet MS;font-size:14;font-weight:bold;color:#26734d&quot;&gt; &lt;br/&gt; Passed Tests: &lt;br/&gt; &lt;br/&gt; &lt;/div&gt;"/>
