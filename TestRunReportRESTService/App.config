<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
  <add key="BuildDefinitions" value="Dev Integration Tests;QA Regression Tests"/>

  <add key="tfsUri" value="https://tatyana-getmanova.visualstudio.com"/>

  <add key="TeamProject" value="TestProject"/>

    <add key="Sender" value="gtd191@gmail.com"/>
    <add key="password" value="haha"/>
