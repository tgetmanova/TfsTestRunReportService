    <add key="HtmlTemplateNotExecutedFailedTestsTitle" value="&lt;div style=&quot;font-family:Trebuchet MS;font-size:14;font-weight:bold;color:#gray&quot;&gt; &lt;br/&gt; These tests have not been executed:  &lt;br/&gt; &lt;br/&gt; &lt;/div&gt;"/>
