    <add key="HtmlTemplateTimeoutFailedTestsTitle" value="&lt;div style=&quot;font-family:Trebuchet MS;font-size:14;font-weight:bold;color:#gray&quot;&gt; &lt;br/&gt; These tests are failed by TimeOut:  &lt;br/&gt; &lt;br/&gt; &lt;/div&gt;"/>
